   ],
   "source": [
    "nShots = 100;\n",
    "\n",
    "pmin = 0.01; pmax = 0.3; pts = 50;\n",
    "p_arr = np.linspace(pmin, pmax, pts);\n",
    "\n",
    "prob_arr = [];\n",
    "for p in p_arr:\n",
    "    bit_flip = pauli_error([('X', p), ('Y', p), ('Z', p), ('I', 1-3*p)])\n",
    "\n",
    "    noise = NoiseModel()\n",
    "    noise.add_all_qubit_quantum_error(bit_flip, ['id'])\n",
    "    noisy_sim = AerSimulator(method='stabilizer', noise_model=noise)\n",
    "    \n",
    "    noisy_circuit = transpile(qc, noisy_sim, optimization_level=0);\n",
    "    \n",
    "    result = noisy_sim.run(noisy_circuit, shots=nShots).result()\n",
    "    counts = result.get_counts()\n",
    "    \n",
    "    success = 0;\n",
    "    for ki in counts.keys():\n",
    "        if ki[2*n+2:2*n+4]==log_state: success+=counts[ki];\n",
    "    \n",
    "    prob_arr.append(success/nShots);\n",
    "\n",
    "plt.scatter(p_arr, prob_arr);\n",
    "plt.xlabel(\"p\");\n",
    "plt.ylabel(\"Success Probability\");\n",
    "plt.ylim(-0.1,1.05);"
   ]
